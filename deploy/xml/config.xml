<config>

	<!--
		APP is implemented to report the API Application SWF name and version
		number.
	-->
	<app>
		<name>Sekati API</name>
		<version>3.1.0.0</version>
	</app>

	<load>
		<!--
			The <preload_application> node is used by the [optional] DocumentPreloader SWF.
			The config.xml is shared by both the preloader and application SWF.
		-->
		<preload_application>sekati_main.swf</preload_application>
		<crossdomain load="true">crossdomain.xml</crossdomain>
		<data load="true">xml/data.xml</data>
		<stylesheet load="true">css/app_style.css</stylesheet>
		<fonts load="true" default="en">
			<font lang="en">assets/FontLibrary.swf</font>
			<font lang="jp">assets/FontLibraryJP.swf</font>
		</fonts>
	</load>

	<option>
		<logger enable="true" with_firebug="true" with_ide="true" />
		<context_menu_management enable="true" with_api_version="true" with_player_version="true" />
		<fullscreen_support enable="true" />
		<browser_management enable="true" />
		<google_analytics enable="true" />
		<performance_monitor enable="true" />
	</option>

	<!--
		CUSTOM is used by the API Application SWF to persist custom
		configuration data as an alternative to the "uri.data" load options.
		To make the custom node data accessible (via "App.db.config.custom")
		you must set <config persist="true">.
	-->
	<custom>
		<!-- 
		<test />
		 -->
	</custom>
</config>
