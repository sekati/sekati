    <id>com.sekati.air.SekatiAirExample</id>
    <version>1.0.0</version>
    <filename>Sekati API Air Example</filename>
    <name>Sekati API Air Example</name>
    <description>An example Air implementation of the Sekati API.</description>
    <copyright>Copyright (C) 2009 jason m horwitz, Sekat LLC. All Rights Reserved.</copyright>
    <initialWindow>
        <title>Sekati API Air Example</title>
        <content>sekati_main.swf</content>
        <!-- 
        <systemChrome>none</systemChrome>
         -->
        <systemChrome>standard</systemChrome>
        <transparent>false</transparent>
        <visible>true</visible>
        <minimizable>true</minimizable>
        <maximizable>false</maximizable>
        <resizable>false</resizable>
        <width>640</width>
        <height>480</height>
        <minSize>320 240</minSize>
        <maxSize>1280 960</maxSize>
		<x>100</x>
		<y>100</y>
    </initialWindow> 
    <installFolder>sekati</installFolder>
    <programMenuFolder>sekati</programMenuFolder>
    <icon>
        <image16x16>icons/icon16.png</image16x16>
        <image32x32>icons/icon32.png</image32x32>
        <image48x48>icons/icon48.png</image48x48>
        <image128x128>icons/icon128.png</image128x128> 
    </icon>
    <customUpdateUI>true</customUpdateUI>
    <allowBrowserInvocation>false</allowBrowserInvocation>
    <fileTypes>
        <fileType>
            <name>adobe.VideoFile</name>
            <extension>avf</extension>
            <description>Adobe Video File</description>
            <contentType>application/vnd.adobe.video-file</contentType>
            <icon>
		        <image16x16>icons/icon16.png</image16x16>
		        <image32x32>icons/icon32.png</image32x32>
		        <image48x48>icons/icon48.png</image48x48>
		        <image128x128>icons/icon128.png</image128x128> 
            </icon>
        </fileType>
    </fileTypes>
</application>