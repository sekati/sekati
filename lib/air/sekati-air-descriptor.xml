<?xml version="1.0" encoding="UTF-8"?>
<application xmlns="http://ns.adobe.com/air/application/1.5" minimumPatchLevel="1047">
	<!--  
		Sekati API AIR Application Descriptor Template 
		@author		Jason M Horwitz
		@website	http://sekati.googlecode.com
		@date		10.30.2009
		@see 		http://livedocs.adobe.com/flex/3/html/help.html?content=File_formats_1.html
		@see 		http://www.adobe.com/go/learn_air_1.0_application_descriptor_en
		@see 		http://labs.mstudio.com/?p=226
