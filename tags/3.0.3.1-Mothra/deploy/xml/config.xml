<config persist="false">
	<app>
		<name><![CDATA[Sekati API]]></name>
		<version>3.0.3.1</version>
	</app>
	<uri>
		<crossdomain load="true">/crossdomain.xml</crossdomain>
		<data load="false">xml/data.xml</data>
		<css load="false">css/app_style.css</css>
	</uri>
	<option>
		<managed_menu>true</managed_menu>
		<api_credit>true</api_credit>
		<fullscreen>true</fullscreen>
		<track>true</track>
		<key>true</key>
		<deeplink>false</deeplink>
		<log>true</log>
		<performance_monitor>false</performance_monitor>
	</option>
	<custom>
		<!-- 
		If you have a small amount of configuration data you wish to persist 
		but do not want to bother with a second (data) XML file you may use
		this "custom" node to populate your data.
		
		In order to access custom node data via the API you must set <config 
		persist="true">; custom will be available via App.db.config.custom.
		-->
	</custom>
</config>
