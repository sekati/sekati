<config persist="false">
	<app>
		<name><![CDATA[Sekati API]]></name>
		<version>3.0.3.3</version>
	</app>
	<uri>
		<crossdomain load="true">crossdomain.xml</crossdomain>
		<data load="true">xml/data.xml</data>
		<css load="true">css/app_style.css</css>
	</uri>
	<option>
		<!-- Log Modes: ALL (true || 3), FIREBUG (2), IDE (1), NONE (false || 0 || ???) -->
		<log>ALL</log>
		<managed_menu>true</managed_menu>
		<api_credit>true</api_credit>
		<fullscreen>true</fullscreen>
		<key>true</key>
		<track>true</track>
		<deeplink>false</deeplink>
		<performance_monitor>true</performance_monitor>
	</option>
	<custom>
		<!-- 
		If you have a small amount of application data you wish to persist but do not 
		want to load a 2nd (data) XML you may use the "custom" node for your data.
		
		In order to access custom node data via the API you must set: 
		<config persist="true"> to make App.db.config.custom available.
		-->	
	</custom>
</config>
